class Curr_acct extends Account\{\par
    private double balance;\par
    private boolean checkBook;\par
    private static double minBalance;\par
    private static double serviceCharge;\par
    public static double getServiceCharge()\{\par
        return serviceCharge;\par
    \}\par
    public Curr_acct(String name,int accountNumber,String type,boolean checkBook)\{\par
        super(name,accountNumber,type);\par
        this.checkBook=checkBook;\par
        this.balance=0;\par
    \}\par
    static\{\par
        minBalance=1000.00;\par
        serviceCharge=5.00;\par
    \}\par
    public double getBalance()\{\par
        return this.balance;\par
    \}\par
    public void deposit(double amt)\{\par
        this.balance+=amt;\par
    \}\par
    public int withdraw(double amt)\{\par
        if(this.balance-amt<minBalance && this.balance-amt>0)\{\par
            this.balance-=serviceCharge*0.01*amt;\par
            this.balance-=amt;\par
            return 1;\par
        \}if(this.balance-amt<0)\{\par
            return -1;\par
        \}\par
        this.balance-=amt;\par
        return 2;\par
    \}\par
\}\par
class Sav_acct extends Account\{\par
    private double balance;\par
    private static double interestRate;\par
    public Sav_acct(String name,int accountNumber,String type)\{\par
        super(name,accountNumber,type);\par
        this.balance=0;\par
    \}\par
    static\{\par
        interestRate=8.0;\par
    \}\par
    public double getBalance()\{\par
        return this.balance;\par
    \}\par
    public void deposit(double amt)\{\par
        this.balance+=amt;\par
    \}\par
    public int withdraw(double amt)\{\par
        if(this.balance-amt<0)\{\par
            return -1;\par
        \}\par
        this.balance-=amt;\par
        return 2;\par
    \}\par
    public double calculateInterest()\{\par
        double amt=(this.balance*(1.0+(interestRate*0.01)));\par
        double interest=amt-this.balance;\par
        this.balance=amt;\par
        return interest;\par
    \}\par
\}\par
